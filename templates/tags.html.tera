<!doctype html>
<html>
    <head>
        <title>All bookmarks</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <style>
            {% include "style.css" %}
        </style>
    </head>
    <body>
        <header>
            <nav>
                <h1>Tags {{ tags|length }}</h1>
                <ul>
                    <li> <a href="/">All</a> </li>
                    <li> <a href="/cat">Categories</a> </li>
                    <li> <a href="/about">About</a> </li>
                </ul>
            </nav>
        </header>
        <main>
            {% for tag in tags %}
            <div>tag.tag_name</div>
            {% endfor %}
        </main>
    </body>
</html>
